
@{
    ViewBag.Title = "Agregar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Scripts/APP/Deporte/Deporte.js"></script>
<h2>Agregar Deporte</h2>

<hr width=100% align="right" size=200 color="black">

<div class="row m-0">

        <hr width=100% align="right" size=200 color="black">

    <div class="row m-0">

        <form id="FormNutricionDeportiva" name="FormNutricionDeportiva">

            <div class="row m-0">
                <div class="col-xs-12 col-sm-12 col-md-12">
                    <label><strong>Codigo Deporte</strong></label>
                    <input style="border: black; border-width: 1px; border-style: solid;" class="form-control" id="CodDeporte" name="CodDeporte" />
                </div>

                <div class="col-xs-12 col-sm-12 col-md-12">
                    <label><strong>Nombre Deporte</strong></label>
                    <input style="border: black; border-width: 1px; border-style: solid;" class="form-control" id="NombreDeporte" name="NombreDeporte" />
                </div>
                

            </div>
        </form>


    </div>

    <div class="col-xs-12 col-sm-12 col-md-12">
        <button type="button" onclick="Createobj()" class="btn btn-block btn-outline-primary btn-rounded btn-outline mt-4" id="SaveDeporte">Guardar</button>
    </div>
</div>
