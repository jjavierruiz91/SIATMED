
@{
    ViewBag.Title = "Agregar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script defer src="~/Scripts/APP/Medicos/Medicos.js"></script>
<h2><strong>Registro Especialistas</strong></h2>

<hr width=100% align="right" size=200 color="black">

<div class="row m-0">

    <form id="FormMedicosDeportiva" name="FormMedicosDeportiva">

        <div class="row m-0">
            <div class="col-xs-4 col-sm-4 col-md-4">
                <label><strong>Número Identificación</strong></label>
                <input style="border: black; border-width: 1px; border-style: solid;" class="form-control" id="CodMedicos" name="CodMedicos" />
            </div>
            <div class="col-xs-4 col-sm-4 col-md-4">
                <label><strong>Estado Especialistas</strong></label>
                <select style="border: black; border-width: 1px; border-style: solid;" class="select2 form-control" id="EstadoEspecialistas" name="EstadoEspecialistas">
                    <option value="">Seleccionar</option>
                    <option value="ACTIVO">ACTIVO</option>
                    <option value="INACTIVO">INACTIVO</option>
                </select>
            </div>
            
            <div class="col-xs-4 col-sm-4 col-md-4">
                <label><strong>Primer Nombre</strong></label>
                <input style="border: black; border-width: 1px; border-style: solid;" class="form-control" id="PrimerNombre" name="PrimerNombre" />
            </div>
            <div class="col-xs-4 col-sm-4 col-md-4">
                <label><strong>Segundo Nombre</strong></label>
                <input style="border: black; border-width: 1px; border-style: solid;" class="form-control" id="SegundoNombre" name="SegundoNombreSegundoNombre" />
            </div>
            <div class="col-xs-4 col-sm-4 col-md-4">
                <label><strong>Primer Apellido</strong></label>
                <input style="border: black; border-width: 1px; border-style: solid;" class="form-control" id="PrimerApellido" name="PrimerApellido" />
            </div>
            <div class="col-xs-4 col-sm-4 col-md-4">
                <label><strong>Segundo Apellido</strong></label>
                <input style="border: black; border-width: 1px; border-style: solid;" class="form-control" id="SegundoApellido" name="SegundoApellido" />
            </div>

            <div class="col-xs-4 col-sm-4 col-md-4">
                <label><strong>Fecha Nacimiento</strong></label>
                <input style="border: black; border-width: 1px; border-style: solid;" type="date" class="form-control" id="FechaNacimiento" name="FechaNacimiento" />
            </div>
            <div class="col-xs-4 col-sm-4 col-md-4">
                <label><strong>Especialidad</strong></label>
                <input style="border: black; border-width: 1px; border-style: solid;" class="form-control" id="Especialidad" name="Especialidad" />
            </div>
            <div class="col-xs-4 col-sm-4 col-md-4">
                <label><strong>Direccción</strong></label>
                <input style="border: black; border-width: 1px; border-style: solid;" class="form-control" id="Direcccion" name="Direcccion" />
            </div>
            <div class="col-xs-4 col-sm-4 col-md-4">
                <label><strong>Telefono</strong>   </label>
                <input style="border: black; border-width: 1px; border-style: solid;" class="form-control" id="Telefono" onkeypress="return soloNumeros(event)" name="Telefono" />
            </div>

            <div class="col-xs-4 col-l-4 col-md-4">
                <label><strong>Centro</strong></label>
                <input style="border: black; border-width: 1px; border-style: solid;" type="text" class="form-control" id="Centro" name="Centro" />
            </div>
            <div class="col-xs-4 col-sm-4 col-md-4">
                <label><strong>Género</strong></label>
                <select style="border: black; border-width: 1px; border-style: solid;" class="select2 form-control" id="Genero" name="Genero">
                    <option value="">Seleccionar</option>
                    <option value="MASCULINO">MASCULINO</option>
                    <option value="FEMENINO">FEMENINO</option>
                </select>
            </div>
            <div class="col-xs-4 col-sm-4 col-md-4">
                <label><strong>Correo</strong></label>
                <input style="border: black; border-width: 1px; border-style: solid;" type="text" class="form-control" id="Correo" name="Correo" />
            </div>

        </div>
    </form>

    <div class="col-xs-4 col-sm-4 col-md-4">
        <button type="button" onclick="Createobj()" class="btn btn-block btn-outline-primary btn-rounded btn-outline mt-4" id="SaveMedicos">Guardar</button>
    </div>
</div>




