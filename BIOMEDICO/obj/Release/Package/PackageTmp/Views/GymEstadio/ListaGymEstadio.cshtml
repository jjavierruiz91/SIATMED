
@{
    ViewBag.Title = "ListaGymEstadio";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    h1 {
        text-align: center;
        color: cadetblue
    }
</style>
<script defer src="~/Scripts/APP/HorarioEntrenadores/ListaHorarioEntrenadores.js"></script>
<h1><strong>Listado Horario Entrenamiento Entrenadores</strong> </h1>

<div class="wrapper wrapper-content">
    <div class="row animated fadeInDown">

        <div class="col-lg-12">
            <div class="ibox ">
                <div class="ibox-content">
                    <label id="Deporte"></label>
                    <div id="calendar" class="fc fc-unthemed fc-ltr">
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="ShowInfo" class="modal inmodal fade" tabindex="-1" role="dialog" aria-hidden="true">
</div>

<div class="table-responsive row col-xl-12 ">
    <table class="table" id="datatable-PlanGymColiseo">
        <thead>
            <tr class="ColorTabla">

                <th>DEPORTE</th>
                <th>NOMBRE ENTRENADOR </th>
                <th>TELEFONO ENTRENADOR </th>
                <th>ESCENARIOS</th>
                <th>HORA INICIO</th>
                <th>HORA FIN</th>
                </tr>
        </thead>
    </table>
</div>

